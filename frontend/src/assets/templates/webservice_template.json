{
  "webservice": "/{ws-name}/{pathname}",
  "db": "None",
  "returnTemplate": "{ws-name}/{ws-name}.json.ftl",
  "errorType": "jsonWithStatus",
  "inputs": [
    "q"
  ],
  "latency": "1000",
  "maxRequests": "2000",
  "where": [
    [
      "?subject",
      "predicate",
      "$q"
    ]
  ],
  "type": "precise",
  "maxRequestsPerMinute": "60"
}
